	$('#pay').css("cssText","opacity:1 !important");
	$("#pay_msg").html('Payment Successful');
	break;
	case "Payment Declined":
	$('#pay').css("cssText","opacity:1 !important");
	$("#pay_msg").html('Payment Declined');
	break;
}

</script>
</html>
