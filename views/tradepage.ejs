	<div class="container">
		<div class="page-header text-center">
			<h1 class="head">Trade Tracker</h1>
		</div>

		<div class="row">
			<div class="col-sm-2">
			</div>
			<div class="col-sm-9">
				<div class="well" style="width: 1000px;margin-left:-80px;">

				<div id="QNUdemo">
					<div style="width:0px;height:82%;border-right:1px solid lightgray;position:absolute;right:298px;"></div>
					<div class="row" id="quotation_row">
						<div id="qr">
							<div class="col-sm-2">
